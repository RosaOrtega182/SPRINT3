<%- include ('../parts/header')%>
  
    


<div class="container mt-3">
<h1 class="page-title text-center">Habilidades t√©cnicas y blandas</h1>
<br>

  

<% if (cantidad > 0) { %>
    <table class="table table-striped alignmiddle mt-3">
        <thead>
            <tr class="home">
                <th>Nombre</th>
                <th>Email</th>
                <th>Editar</th>
               
            </tr>
        </thead>
        <tbody>
            <% todosLosUsuarios.forEach(function(users){ %>
            <tr>
                <td><%= users.nombre %></td>
                <td><%= users.email %></td>
             

                <td><a href="/admin/skills/editProfile/<%= users.id %>"><i class="fas fa-user-edit"></i>&nbspEditar</a></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
    <% } else {%>
    <h3 class="text-center">No hay usuarios.</h3>
    <% }%>



  </div>





<!-- BOOTSTRAP 5 JS-->
